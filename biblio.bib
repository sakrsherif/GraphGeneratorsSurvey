@inproceedings{wang2013,
  author={Wang, Chong Jun and Wang, Gang and Lei, Yu Li and Qiao, Shao Jie},
  title={Community Evolution in Dynamic Social Networks},
  year={2013},
  month={10},
  volume={756},
  pages={2634--2638},
  booktitle={Information Technology Applications in Industry, Computer Engineering and Materials Science},
